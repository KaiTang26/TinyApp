
<!-- this is head  -->
<% if(username){%>

<form action="/logout" method="POST" style="margin:10px;">
  <label>Current User: <%= username %></label>
  <input type="submit" value="Logout">
</form>

<%} else{%>

<form action="/login" method="POST" style="margin:10px;">
  <label for="username">UserName:</label>
      <input id="username" type="text" name="username" placeholder="Please input username" style="width: 300px">
      <input type="submit" value="Submit">
</form>

<%}%>


